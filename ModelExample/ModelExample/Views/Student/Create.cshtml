
@{
    ViewBag.Title = "Create";
}

<h2>Create Student </h2>



<form method="post" action="/Student/Create">
    <div class="form-group">
        <label>Student Id</label>
        <input type="text" class="form-control" id="txtStudentId" name="StudentId" placeholder="Enter Student id">
    </div>
    <div class="form-group">
        <label>Student Name</label>
        <input type="text" class="form-control" id="txtStudentName" name="StudentName" placeholder="Enter Student name">
    </div>
    <div class="form-group">
        <label>D.No</label>
        <input type="text" class="form-control" id="txtdNo" name="Dno" placeholder="Enter D.No">
    </div>
    <div class="form-group">
        <label>Street</label>
        <input type="text" class="form-control" id="txtsteet" name="Street" placeholder="Enter street">
    </div>
    <div class="form-group">
        <label>Landmark</label>
        <input type="text" class="form-control" id="txtLandmark" name="Landmark" placeholder="Enter landmark">
    </div>
    <div class="form-group">
        <label>City</label>
        <input type="text" class="form-control" id="txtCity" name="City" placeholder="Enter City">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>


