
@{
    ViewBag.Title = "Create";
}

<h2>Create Product</h2>



<form method="post" action="/Products/Create">
    <div class="form-group">
        <label>Product Id</label>
        <input type="text" class="form-control" id="txtproductId" name="ProductId" placeholder="Enter product id">
    </div>
    <div class="form-group">
        <label>Product Name</label>
        <input type="text" class="form-control" id="txtproductName"  name="ProductName" placeholder="Enter product name">
    </div>
    <div class="form-group">
        <label>Product Rate</label>
        <input type="text" class="form-control" id="txtproductRate"  name="Rate"  placeholder="Enter product rate">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>


