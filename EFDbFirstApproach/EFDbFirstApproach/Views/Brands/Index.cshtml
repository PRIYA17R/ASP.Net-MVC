@model List<EFDbFirstApproach.Models.Brand>
@{
    ViewBag.Title = "Brands";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Brands </h2>
<table class="table">
    <tr>
        <th>Brand ID</th>
        <th>Brand Name </th>
    </tr>
    @foreach (var brand in Model)
    {
        <tr>

            <td>@brand.BrandID</td>
            <td>@brand.BrandName</td>

        </tr>
    }
</table>

