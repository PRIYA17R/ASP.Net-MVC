@model List<EFDbFirstApproach.Models.Category>
@{
    ViewBag.Title = "Categories";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Categories</h2>

<table class="table">
    <tr>
        <th>Category ID</th>
        <th>Category Name </th>
    </tr>
    @foreach (var category in Model)
    {
        <tr>

            <td>@category.CategoryID</td>
            <td>@category.CategoryName</td>
            
        </tr>
    }
    </table>