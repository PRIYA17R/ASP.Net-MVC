@model List<EFDbFirstApproach.Models.Product>
@{
    ViewBag.Title = "Product";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}
<h2>Products </h2>
<table class="table">
    <tr>
        <th>Product ID</th>
        <th>Product Name </th>

        <th>Product Price</th>
        <th>Date of Purchase </th>

        <th>Avaibility Status</th>
        <th>CategoryID </th>

        <th>Brand ID</th>
    </tr>
    @foreach (var product in Model)
    {
<tr>

    <td>@product.ProductID</td>
    <td>@product.ProductName</td>
    <td>@product.Price</td>
    <td>@product.DateOfPurchase</td>
    <td>@product.AvailabilityStatus</td>
    <td>@product.CategoryID</td>
    <td>@product.BrandID</td>

</tr>
    }
</table>

